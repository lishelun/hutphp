  "license": "MIT",
  "authors": [
    {
      "name": "Lishelun",
      "email": "lishelun@qq.com"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "topthink/framework": "^6.0",
    "topthink/think-captcha": "^3.0",
    "topthink/think-multi-app": "^1.0",
    "ext-iconv": "*",
    "ext-gd": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "firebase/php-jwt": "^5.4"
  },
  "autoload": {
    "files": [
      "src/common.php"
    ],
    "psr-4": {
